{
  "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "import/prefer-default-export": "off",
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
      "import/extensions": [
        "error",
        {
            "js": "ignorePackages"
        }
    ],
    "no-undef": ["warning", { "typeof": false }]
    }
  }